{
  "name": "electron-updater-generic-example",
  "version": "0.4.0",
  "scripts": {
    "start": "electron .",
    "packager:win": "electron-packager  ./app autoUpdate --platform=win32 --arch=x64 --out=./outapp --overwrite",
    "build:win": "build --platform win32 --arch=x64"
  },
  "main": "app/main.js",
  "description": "electron-updater generic example project",
  "author": "Matt Haggard",
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "^19.16.2",
    "http-server": "^0.9.0"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
      }
    ],
    "appId": "com.github.iffy.electronupdatergenericexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
}
